@model ClinicWebApplication.PatientSymptom

@{
    ViewData["Title"] = "Details";
}

<h1>Деталі</h1>

<div>
    <h4>Пацієнт-Симптом</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Time)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Time)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Patient)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Patient.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Symptom)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Symptom.Id)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" asp-route-patientId="@ViewBag.patientId">Редагувати</a> |
    <a asp-action="Index" asp-route-patientId="@ViewBag.patientId">Назад</a>
</div>
